<div class="card">
<%= link_to builder.user.name, user_path(builder.user), class: :card__user %>
  <%= link_to image_tag(builder.image),builder_path(builder.id),class: :card__img %>
  <div class="card__body">
    <%= link_to builder.title, builder_path(builder.id), class: :card__title%>
    <p class="card__summary">
      <%=builder.category.name%>
    </p>
  </div>
</div>